<html lang="en">
  <head>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta property="og:title" content="Jim Kinsey's Online Gallery"/>
    <title>Gallery</title>
    <link href="{{#furniture}}gallery.css{{/furniture}}" rel="stylesheet"/>
  </head>
  <body>
    <main id="content">
      <header>
        <h1>Gallery</h1>
      </header>
      {{#series}}
      <article id="{{id}}" class="series">
        <header><h2>{{title}}</h2></header>
        <ul>{{#artworks}}<li class="work">
           <a class="artwork-link" href="{{url}}">{{title}}</a><br/>
           <a href="{{url}}"><img id="thumbnail" src="{{thumbnailURL}}"></a>
        </li>{{/artworks}}
        </ul>
      </article>
      {{/series}}
    </main>
    <footer>
      <small id="copyright">&copy; 2013-2021 Jim Kinsey</small>
    </footer>
  </body>
</html>